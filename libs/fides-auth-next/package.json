{
  "name": "@eventuras/fides-auth-next",
  "version": "0.1.0",
  "private": true,
  "description": "Next.js bindings for @eventuras/fides-auth",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./session": {
      "types": "./dist/session.d.ts",
      "import": "./dist/session.js"
    },
    "./request": {
      "types": "./dist/request.d.ts",
      "import": "./dist/request.js"
    },
    "./state-machine": {
      "types": "./dist/state-machine/index.d.ts",
      "import": "./dist/state-machine/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch"
  },
  "dependencies": {
    "@eventuras/fides-auth": "workspace:*",
    "@eventuras/logger": "workspace:*",
    "@eventuras/typescript-config": "workspace:*",
    "next": "^15.5.5",
    "xstate": "^5.23.0"
  },
  "peerDependencies": {
    "@xstate/react": "^5.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@eventuras/vite-config": "workspace:*",
    "vite": "^7.1.10"
  }
}
