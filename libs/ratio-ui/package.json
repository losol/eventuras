{
  "name": "@eventuras/ratio-ui",
  "version": "0.1.1",
  "type": "module",
  "main": "dist/eventuras-ui.umd.js",
  "module": "dist/eventuras-ui.es.js",
  "types": "dist/index.d.ts",
  "style": "dist/ui.css",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/eventuras-ui.es.js",
      "types": "./dist/index.d.ts"
    },
    "./forms/*": "./forms/*",
    "./ratio-ui.css": "./dist/ratio-ui.css"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "scripts": {
    "dev": "npm run storybook",
    "build": "tsc && vite build && storybook build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "ajv": "^8.17.1",
    "lucide-react": "^0.539.0",
    "react-aria-components": "^1.11.0",
    "tailwindcss-react-aria-components": "^2.0.0"
  },
  "devDependencies": {
    "@eventuras/eslint-config": "*",
    "@eventuras/typescript-config": "*",
    "@storybook/addon-a11y": "^9.1.2",
    "@storybook/addon-docs": "^9.1.2",
    "@storybook/addon-onboarding": "^9.1.2",
    "@storybook/addon-themes": "^9.1.2",
    "@storybook/react-vite": "^9.1.2",
    "@types/react": "^19.1.10",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "10.4.21",
    "eslint-plugin-storybook": "^9.1.2",
    "glob": "11.0.3",
    "postcss": "^8.5.6",
    "storybook": "^9.1.2",
    "tailwindcss": "^4.1.11",
    "typescript": "5.9.2",
    "vite-plugin-dts": "4.5.4"
  },
  "overrides": {
    "vite": {
      "rollup": "npm:@rollup/wasm-node"
    },
    "storybook": "$storybook"
  }
}
