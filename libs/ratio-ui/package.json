{
  "name": "@eventuras/ratio-ui",
  "version": "0.6.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./core/*": {
      "import": "./dist/core/*/index.js",
      "types": "./dist/core/*/index.d.ts"
    },
    "./layout/*": {
      "import": "./dist/layout/*/index.js",
      "types": "./dist/layout/*/index.d.ts"
    },
    "./forms": {
      "import": "./dist/forms/index.js",
      "types": "./dist/forms/index.d.ts"
    },
    "./forms/*": {
      "import": "./dist/forms/*/index.js",
      "types": "./dist/forms/*/index.d.ts"
    },
    "./pages/*": {
      "import": "./dist/pages/*/index.js",
      "types": "./dist/pages/*/index.d.ts"
    },
    "./blocks/*": {
      "import": "./dist/blocks/*/index.js",
      "types": "./dist/blocks/*/index.d.ts"
    },
    "./ratio-ui.css": "./dist/ratio-ui.css"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "build:all": "pnpm run build && pnpm run storybook:build",
    "dev": "pnpm run storybook",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "overrides": {
    "storybook": "$storybook",
    "vite": {
      "rollup": "npm:@rollup/wasm-node"
    }
  },
  "dependencies": {
    "@eventuras/logger": "workspace:*",
    "@storybook/react": "^9.1.10",
    "@tabler/icons-react": "^3.35.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/vite": "^4.1.14",
    "@react-aria/overlays": "^3.26.0",
    "ajv": "^8.17.1",
    "lucide-react": "^0.545.0",
    "react-aria-components": "^1.13.0",
    "react-stately": "^3.35.0",
    "tailwindcss-react-aria-components": "^2.0.1"
  },
  "devDependencies": {
    "@eventuras/eslint-config": "workspace:*",
    "@eventuras/typescript-config": "workspace:*",
    "@eventuras/vite-config": "workspace:*",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-onboarding": "^9.1.10",
    "@storybook/addon-themes": "^9.1.10",
    "@storybook/react-vite": "^9.1.10",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "autoprefixer": "10.4.21",
    "clsx": "^2.1.1",
    "eslint-plugin-storybook": "^9.1.10",
    "postcss": "^8.5.6",
    "react-dom": "^19.2.0",
    "storybook": "^9.1.10",
    "tailwindcss": "^4.1.14",
    "typescript": "5.9.3",
    "vite": "7.1.10"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
