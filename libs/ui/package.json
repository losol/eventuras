{
  "name": "@eventuras/ui",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/eventuras-ui.umd.js",
  "module": "dist/eventuras-ui.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/eventuras-ui.es.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": {
      "import": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:css": "tailwindcss -i ./src/styles/tailwind.css -o ./dist/styles.css",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "devDependencies": {
    "@eventuras/eslint-config": "*",
    "@eventuras/typescript-config": "*",
    "@storybook/addon-a11y": "^8.0.4",
    "@storybook/addon-console": "^3.0.0",
    "@storybook/addon-docs": "^8.0.4",
    "@storybook/addon-essentials": "^8.0.4",
    "@storybook/addon-interactions": "^8.0.4",
    "@storybook/addon-links": "^8.0.4",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^8.0.4",
    "@storybook/react": "^8.0.4",
    "@storybook/react-vite": "^8.0.5",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/testing-react": "^2.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "glob": "^10.3.12",
    "postcss": "^8.4.38",
    "storybook": "^8.0.4",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.3",
    "vite-plugin-dts": "^3.8.1"
  },
  "overrides": {
    "vite": {
      "rollup": "npm:@rollup/wasm-node"
    }
  }
}
