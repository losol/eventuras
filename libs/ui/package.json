{
  "name": "@eventuras/ui",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/eventuras-ui.umd.js",
  "module": "dist/eventuras-ui.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/eventuras-ui.es.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": {
      "import": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:css": "tailwindcss -i ./src/styles/tailwind.css -o ./dist/styles.css",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "ajv": "^6.12.6",
    "react-aria-components": "^1.7.1"
  },
  "devDependencies": {
    "@eventuras/eslint-config": "*",
    "@eventuras/typescript-config": "*",
    "@storybook/addon-a11y": "8.6.12",
    "@storybook/addon-console": "3.0.0",
    "@storybook/addon-docs": "8.6.12",
    "@storybook/addon-essentials": "8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-links": "8.6.12",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/blocks": "^8.6.12",
    "@storybook/react": "8.6.12",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "10.4.21",
    "glob": "11.0.1",
    "postcss": "8.5.3",
    "storybook": "8.6.12",
    "tailwindcss": "3.4.17",
    "typescript": "5.8.3",
    "vite-plugin-dts": "4.5.3"
  },
  "overrides": {
    "vite": {
      "rollup": "npm:@rollup/wasm-node"
    }
  }
}
