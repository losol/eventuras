{
  "name": "@eventuras/vipps",
  "version": "0.1.1",
  "private": true,
  "description": "Vipps MobilePay integration for Eventuras",
  "type": "module",
  "exports": {
    "./epayment-v1": {
      "types": "./dist/epayment-v1/index.d.ts",
      "import": "./dist/epayment-v1/index.js"
    },
    "./vipps-core": {
      "types": "./dist/vipps-core/index.d.ts",
      "import": "./dist/vipps-core/index.js"
    },
    "./webhooks-v1": {
      "types": "./dist/webhooks-v1/index.d.ts",
      "import": "./dist/webhooks-v1/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "webhook:delete": "tsx --env-file=.env scripts/delete-webhook.ts",
    "webhook:list": "tsx --env-file=.env scripts/list-webhooks.ts",
    "webhook:setup": "tsx --env-file=.env scripts/setup-webhook.ts"
  },
  "dependencies": {
    "@eventuras/logger": "workspace:*"
  },
  "devDependencies": {
    "@eventuras/typescript-config": "workspace:*",
    "@eventuras/vite-config": "workspace:*",
    "@vitest/ui": "^3.0.5",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vitest": "^3.0.5"
  }
}
