{
  "name": "@eventuras/historia",
  "version": "0.4.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && npm run build && npm run start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation npm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm package-lock.json && npm --no-workspaces --force install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.43.0",
    "@payloadcms/db-sqlite": "^3.43.0",
    "@payloadcms/live-preview-react": "3.43.0",
    "@payloadcms/next": "3.43.0",
    "@payloadcms/payload-cloud": "3.43.0",
    "@payloadcms/plugin-form-builder": "3.43.0",
    "@payloadcms/plugin-nested-docs": "3.43.0",
    "@payloadcms/plugin-redirects": "3.43.0",
    "@payloadcms/plugin-search": "3.43.0",
    "@payloadcms/plugin-seo": "3.43.0",
    "@payloadcms/richtext-lexical": "3.43.0",
    "@payloadcms/storage-s3": "^3.43.0",
    "@payloadcms/ui": "3.42.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "aws-crt": "^1.27.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "geist": "^1.4.2",
    "graphql": "^16.11.0",
    "lucide-react": "^0.515.0",
    "nanoid": "^5.1.5",
    "next": "^15.3.3",
    "next-sitemap": "^4.2.3",
    "payload": "3.43.0",
    "payload-admin-bar": "^1.0.7",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "7.58.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.10",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "24.0.3",
    "@types/react": "19.1.8",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "optionalDependencies": {
    "@esbuild/linux-x64": "0.25.5",
    "@libsql/linux-x64-gnu": "0.5.13",
    "sharp": "^0.34.2"
  }
}
