{
  "extends": "@eventuras/typescript-config/nextjs",
  "compilerOptions": {
    "paths": {
      "@/*": ["./src/*", "./public/*"],
      "@eventuras/datatable": ["../../libs/datatable/src/*"],
      "@eventuras/enrollments-sdk": ["../../libs/enrollments-sdk"],
      "@eventuras/fides-auth/*": ["../../libs/fides-auth/src/*"],
      "@eventuras/forms*": ["../../libs/forms/src*"],
      "@eventuras/markdown": ["../../libs/markdown/src/*"],
      "@eventuras/markdowninput": ["../../libs/markdowninput"],
      "@eventuras/scribo": ["../../libs/scribo"],
      "@eventuras/smartform": ["../../libs/smartform"],
      "@eventuras/sdk": ["../../libs/sdk"],
      "@eventuras/ratio-ui": ["../../libs/ratio-ui/src"],
      "@eventuras/ratio-uitio-ui/*": ["../../libs/ratio-ui/src/*"],
      "@eventuras/utils": ["../../libs/utils"],
      "@eventuras/utils/*": ["../../libs/utils/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "i18n.js",
    "tailwind.config.js",
    "postcss.config.js",
    "next-env.d.ts",
    "next.config.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "../../libs/scribo/src/ui/DropDown.tsx"
  ],
  "exclude": ["node_modules"]
}
