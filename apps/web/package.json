{
  "name": "@eventuras/web",
  "version": "0.6.1",
  "private": true,
  "type": "module",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "vercel-build": "pnpm --filter=@eventuras/web^... build && next build",
    "dev": "next dev",
    "generate:config-types": "tsx scripts/generate-config-types.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix && prettier . --write",
    "start": "next start"
  },
  "lint-staged": {
    "**/*.(ts|tsx|js)": [
      "eslint --fix",
      "prettier --cache --write"
    ],
    "*.{css,json}": "prettier --cache --write"
  },
  "browserslist": [
    "defaults, not dead"
  ],
  "dependencies": {
    "@eventuras/app-config": "workspace:^",
    "@eventuras/core": "workspace:^",
    "@eventuras/datatable": "workspace:^",
    "@eventuras/event-sdk": "workspace:^",
    "@eventuras/fides-auth-next": "workspace:*",
    "@eventuras/logger": "workspace:^",
    "@eventuras/markdown": "workspace:*",
    "@eventuras/markdowninput": "workspace:^",
    "@eventuras/ratio-ui": "workspace:*",
    "@eventuras/ratio-ui-next": "workspace:*",
    "@eventuras/scribo": "workspace:*",
    "@eventuras/sdk": "workspace:*",
    "@eventuras/smartform": "workspace:*",
    "@eventuras/toast": "workspace:^",
    "@tabler/icons-react": "^3.35.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/debug": "^4.1.12",
    "@xstate/react": "^6.0.0",
    "jose": "6.1.0",
    "lucide-react": "^0.545.0",
    "next": "15.5.5",
    "next-intl": "^4.3.12",
    "openid-client": "^6.8.1",
    "pino": "^10.0.0",
    "react": "^19.2.0",
    "react-aria-components": "^1.13.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "tailwindcss-react-aria-components": "^2.0.1",
    "xstate": "^5.23.0"
  },
  "devDependencies": {
    "@eventuras/eslint-config": "workspace:*",
    "@next/bundle-analyzer": "15.5.5",
    "@types/node": "24.0.3",
    "@types/react": "19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.14",
    "typescript": "5.9.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.52.4"
  },
  "engines": {
    "node": ">=22.0.0 <25.0.0",
    "pnpm": ">=9.0.0"
  }
}
