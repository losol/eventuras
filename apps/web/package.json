    "test:playwright:skiplogin": "playwright test --project=skip-login --workers=1",
    "test:playwright:ui": "playwright test --ui",
    "chromatic": "npx chromatic --project-token chpt_39f4ebd3d21130e"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/server": "^11.11.0",
    "@headlessui/react": "^1.7.18",
    "@losol/eventuras": "^0.6.9",
    "@nx/next": "^17.3.0",
    "@eventuras/scribo": "*",
    "@sentry/nextjs": "^7.99.0",
    "@tabler/icons-react": "^2.46.0",
    "@tanstack/match-sorter-utils": "^8.11.7",
    "@tanstack/react-table": "^8.11.7",
    "@xstate/react": "^4.0.3",
    "debug": "^4.3.4",
    "dotenv": "^16.4.1",
    "html-react-parser": "^5.1.1",
    "i18n": "^0.15.1",
    "jwt-decode": "^4.0.0",
    "lexical": "^0.12.6",
    "markdown-to-jsx": "^7.4.1",
