{
  "name": "@eventuras/oxo",
  "version": "0.0.1",
  "description": "Eventuras CLI tools",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/losol/eventuras",
  "bugs": "https://github.com/losol/eventuras/issues",
  "repository": "losol/eventuras",
  "license": "MIT",
  "author": "Ole Kristian Losvik",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "oxo": "./bin/run.js"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint .",
    "prepack": "oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "test": "vitest run",
    "posttest": "pnpm run lint",
    "test:watch": "vitest",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "oxo",
    "commands": "./dist/commands",
    "dirname": "oxo",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "shipper": {
        "description": "Shipping and logistics integration commands"
      }
    }
  },
  "dependencies": {
    "@eventuras/shipper": "workspace:*",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5"
  },
  "devDependencies": {
    "@eslint/compat": "^2",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/node": "^18",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "oclif": "^4",
    "shx": "^0.4.0",
    "ts-node": "^10",
    "typescript": "^5",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=22.0.0 <25.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
