{
  "name": "eventuras",
  "version": "0.1.0",
  "license": "GPL-2.0+",
  "private": true,
  "packageManager": "npm@10.9.0",
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eventuras/eslint-config": "*",
    "@eventuras/typescript-config": "*",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.31.0",
    "prettier": "^3.3.3",
    "turbo": "^2.1.3",
    "typescript": "^5.6.3",
    "husky": "^9.1.6"
  },
  "workspaces": [
    "apps/docsite",
    "apps/web",
    "apps/web-e2e",
    "apps/convertoapi",
    "libs/**"
  ],
  "engines": {
    "npm": ">=10.0.0 <11.0.0",
    "node": ">=20.0.0 <21.0.0"
  },
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "npm:check-update": "npx npm-check-updates --deep",
    "npm:update": "npx npm-check-updates --deep -u",
    "npm:clean": "npm exec --workspaces -- npx rimraf node_modules && npx rimraf node_modules && npx rimraf package-lock.json",
    "prepare": "husky"
  },
  "dependencies": {
    "next": "^14.2.15",
    "nextra": "^3.0.11",
    "nextra-theme-docs": "^3.0.11",
    "piscina": "^4.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
